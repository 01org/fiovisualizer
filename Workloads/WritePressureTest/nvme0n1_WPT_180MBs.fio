[global]
thread
direct=1
group_reporting
norandommap=1
randrepeat=0
refill_buffers
ioengine=libaio
gtod_reduce=0
time_based
blocksize=8k
runtime=10
log_avg_msec=500
[Random_Write]
rw=randwrite
numjobs=8
iodepth=8
write_bw_log=nvme0n1_WPT_180MBs
rate=,22500k
filename=/dev/nvme0n1

[Random_Read]
rw=randread
numjobs=1
iodepth=1
write_lat_log=nvme0n1_WPT_180MBs
percentile_list=99:99.9:99.99:99.999:99.9999:99.99999:99.999999:99.9999999
filename=/dev/nvme0n1

